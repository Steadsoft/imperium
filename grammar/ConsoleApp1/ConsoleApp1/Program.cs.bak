    public class Actions<T> where T : notnull
    {
        private Dictionary<T,Action> actions = new Dictionary<T,Action>();
        public void Add(T Key,Action Action)
        {
            actions[Key] = Action;
        }
        public Action this[T Index]
        {
            get 
            { 
                return actions[Index];
            }
        }
    }
    
